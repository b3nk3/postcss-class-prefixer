{
  "name": "postcss-class-prefixer",
  "version": "0.0.0",
  "description": "PostCSS plugin to prefix all off the classes with a class name of your choosing.",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "postcss-class-prefixer",
    "prefixer",
    "webpack-loader"
  ],
  "main": "dist/index.js",
  "files": [
    "*.md",
    "build"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist",
    "test": "jest-ci --coverage && eslint-ci .",
    "prepublishOnly": "npm run build"
  },
  "author": "Ben Szabo <me@benszabo.co.uk>",
  "license": "MIT",
  "repository": "b3nk3/postcss-class-prefixer",
  "engines": {
    "node": ">=10.0.0"
  },
  "peerDependencies": {
    "postcss": "^8.1.5"
  },
  "devDependencies": {
    "@jest/types": "^26.6.2",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "clean-publish": "^1.1.8",
    "eslint": "^7.19.0",
    "eslint-ci": "^1.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-ci": "^0.1.1",
    "lint-staged": "^10.4.0",
    "postcss": "^8.1.5",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.8",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  }
}
